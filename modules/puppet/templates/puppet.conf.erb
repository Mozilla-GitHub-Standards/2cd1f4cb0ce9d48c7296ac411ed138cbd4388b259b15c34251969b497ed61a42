[main]
    logdir = /var/log/puppet
    rundir = /var/run/puppet
    ssldir = $vardir/ssl
    certname = <%= @certname %>
    dns_alt_names = <%= @certname %>,<%= @server %>
[agent]
    server = <%= @server %>
    classfile = $vardir/classes.txt
    localconfig = $vardir/localconfig
[master]
    autosign = true
