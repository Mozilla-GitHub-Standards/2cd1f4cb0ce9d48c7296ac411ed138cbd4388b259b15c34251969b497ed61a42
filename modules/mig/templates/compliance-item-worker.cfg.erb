[api]
    host = "https://api.mig.mozilla.org"
[mozdef]
    host            = "mozdef-relay.opsec.mozilla.com"
    port            = 5671
    user            = "mig"
    pass            = "REPLACEMOZDEFPASSWORD"
    vhost           = "prod"
    exchange        = "eventtask"
    routingkey      = "eventtask"
    usetls          = true
    cacertpath      = "/etc/mig/ca.crt"
    clientcertpath  = "/etc/mig/worker.crt"
    clientkeypath   = "/etc/mig/worker.key"
    timeout         = "10s"
[mq]
    host    = "publicrelay.mig.mozilla.org"
    port    = 443
    user    = "worker"
    pass    = "REPLACEMQPASSWORD"
    vhost   = "mig"
    usetls  = true
    cacert  = "/etc/mig/ca.crt"
    tlscert = "/etc/mig/worker.crt"
    tlskey  = "/etc/mig/worker.key"
    timeout = "10s"
[vmintgr]
    bin     = "/opt/vmintgr/bin/grouptest"
[logging]
    mode    = "syslog" ; stdout | file | syslog
    level   = "info"
    host    = "localhost"
    port    = 514
    protocol = "udp"
